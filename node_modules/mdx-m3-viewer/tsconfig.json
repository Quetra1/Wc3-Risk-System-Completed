{
  "compilerOptions": {
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "suppressImplicitAnyIndexErrors": true,
    "noImplicitOverride": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "noPropertyAccessFromIndexSignature": true,
    "strict": true,
    "module": "commonjs",
    "lib": [
      "es2017",
      "dom"
    ],
    "target": "es2017",
    "types": [
      "node"
    ],
    "outDir": "./dist"
  }
}